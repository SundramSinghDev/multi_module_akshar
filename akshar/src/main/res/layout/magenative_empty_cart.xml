<layout xmlns:android="http://schemas.android.com/apk/res/android">
    <data xmlns:android="http://schemas.android.com/apk/res/android">
    </data>
    <RelativeLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white">

        <androidx.appcompat.widget.AppCompatTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/noshopping"
            android:textColor="@color/black"
            android:textSize="12sp"
            android:id="@+id/cart"
            android:fontFamily="@font/celias_regular"
            android:layout_above="@+id/conti"
            android:gravity="center"
            android:layout_marginBottom="10dp"
            android:padding="10dp"
            android:layout_centerHorizontal="true"/>

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/conti"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:layout_marginTop="10dp"
            android:background="@drawable/secondary_box"
            android:backgroundTint="#30B7C5"
            android:fontFamily="@font/celias_regular"
            android:gravity="center"
            android:minHeight="40dp"
            android:padding="@dimen/pad_10dp"
            android:text="@string/continueshopping"
            android:textAllCaps="true"
            android:textColor="@color/white"
            android:textSize="12sp" />
<!--        txtsecondory_color-->
    </RelativeLayout>
</layout>